{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.1.0",
  "description": "<%= description %>",
  "author": "<%= user.name %> <<%= user.email %>>",
  <% if (browserify) { %>"main": "scripts/index.js",
  "browser": {
    "boostrap": "./bower_componets/bootstrap/dist/js/bootstrap.js"
  },
  "browserify-shim": {
    "jquery": "global:jQuery",
    "boostrap": {
      "exports": "jQuery",
      "depends": "jquery:jQuery"
    }
  },
  <% } %>
  "repository": {
    "type": "git",
    "url": "<%= repo %>"
  }
}
